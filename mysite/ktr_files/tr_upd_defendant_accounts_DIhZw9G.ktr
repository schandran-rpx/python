    <connection>sf_conn</connection>
    <sql>WITH cte AS
(SELECT DISTINCT
      tmp.id,
      e.id AS entity_id__c,
      e.name AS name,
	  t.name AS entity_type__c
FROM core.ents e
JOIN core.ent_types t ON (t.id = e.ent_type_id)
LEFT JOIN core.ent_subtypes_map esm ON (esm.ent_id = e.id)
LEFT JOIN core.ent_subtypes es ON (es.id = esm.ent_subtype_id)
LEFT JOIN sf_etl_app.sf_main_etl_acct_temp tmp ON (tmp.entity_id__c = e.id OR lower(tmp.name)=lower(e.name))
WHERE e.id = e.ultimate_parent_id
AND COALESCE(es.name, 'X') not in ('NPE','E-Seller','Patent Licensing Company','Defensive Entity')
AND e.ent_type_id = 1
AND e.id IN (SELECT DISTINCT a.ent_id
            FROM  portal_etl_lits.v_lit_parties_staging lp
            JOIN core.aliases A ON (a.id = lp.alias_id)
			JOIN core.lit_annotations la ON (la.lit_id = lp.lit_id)
            WHERE lp.dj_party_normalized_type = 'defendant'
			AND la.lit_type_id  IN (1, 7)
            AND not exists (select 1 from rpx_reporting.all_lits al where al.core_lit_id = lp.lit_id and has_eseller_defendant)
			AND la.lit_curated_cause_type_id IN (1, 7, 8)
		    AND lp.created_at &lt; now() - interval '2 day'
            AND lp.end_date is null
            AND lp.is_verified)
AND tmp.id not in (select unnest(ids) as id from (select lower(name),array_agg(id) as ids from sf_etl_app.sf_main_etl_acct_temp group by lower(name) having count(id) > 1)t)
) SELECT cte.*, t.name as name_old, t.entity_id__c as entity_id__c_old
 FROM cte
LEFT JOIN sf_etl_app.sf_main_etl_acct_temp t ON (t.id = cte.id)
WHERE cte.entity_id__c not in (select entity_id__c from cte group by 1 having count(1) > 1);</sql>
